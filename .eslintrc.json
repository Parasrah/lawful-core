{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"]
  },
  "globals": {
    "game": "writable",
    "Hooks": "readonly"
  }
}
